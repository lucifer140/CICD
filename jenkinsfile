pipeline {
    agent any

    stages {
       stage('build') {
            steps {
                sh '''
               docker build -t jenks -f Dockerfile .
               docker images
               '''
            }
        }
        stage('deploy') {
            steps {
                sh '''
                docker run -d --name junkey -p 8888:8080 jenks
                '''
            }
        }
        
    }
}
